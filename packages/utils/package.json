{
	"name": "@yuwena/utils",
	"version": "1.0.5",
	"description": "A simple utils library",
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"node": "./dist/es/index.js",
				"default": "./dist/es/index.js"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"node": "./dist/umd/index.js",
				"default": "./dist/umd/index.js"
			}
		}
	},
	"types": "dist/index.d.ts",
	"main": "dist/umd/index.js",
	"module": "dist/es/index.js",
	"files": [
		"dist"
	],
	"keywords": [
		"utils",
		"library",
		"typescript",
		"rollup",
		"babel",
		"esm",
		"cjs"
	],
	"homepage": "https://www.moshangl.cn/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yuwenOvO/yuwenUtils.git"
	},
	"bugs": {
		"url": "https://github.com/yuwenOvO/yuwenUtils/issues"
	},
	"engines": {
		"node": ">=20.13.1"
	},
	"packageManager": "pnpm@9.1.0",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"utils:build": "rollup --config",
		"utils:publish": "pnpm run utils:build && npm publish --access public"
	},
	"author": "yuwen",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.24.3",
		"@babel/preset-env": "^7.24.3",
		"@babel/preset-typescript": "^7.24.7",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"rollup": "^4.17.2",
		"rollup-plugin-dts": "^6.1.1"
	}
}
